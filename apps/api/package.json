{
  "name": "kibei-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node ../../scripts/sync-prisma-engine.js && next dev -p 3000",
    "build": "npx prisma generate --schema=../../packages/db/schema.prisma && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate --schema=../../packages/db/schema.prisma",
    "postinstall": "cd ../../packages/db && npm run generate || true"
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@kibei/db": "*",
    "@kibei/auth": "*",
    "@kibei/services": "*",
    "@kibei/config": "*",
    "@kibei/utils": "*",
    "zod": "latest",
    "jose": "latest",
    "@supabase/supabase-js": "latest",
    "@prisma/client": "^6.19.1"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "^14.2.0",
    "@eslint/eslintrc": "^3.1.0",
    "prisma": "^6.19.1"
  }
}
