{
  "name": "kibei",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "db:push": "turbo db:push --filter=@kibei/db",
    "db:migrate": "turbo db:migrate --filter=@kibei/db",
    "db:seed": "turbo db:seed --filter=@kibei/db",
    "setup": "bash scripts/setup.sh"
  },
  "dependencies": {},
  "devDependencies": {
    "turbo": "latest",
    "typescript": "^5.3.3",
    "@tailwindcss/postcss": "^4.0.0"
  },
  "packageManager": "npm@10.0.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
